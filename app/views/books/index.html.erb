books index(/books)

<!--テンプレートファイルを呼び出す-->
<%= render 'list', user: @user %>

<!--ここから右ページ-->
<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th colspan="1"></th>
      <th>
        title
      </th>
      <th>
        body
      </th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td>
      <%= image_tag book.user.get_image(100,100) %>
      </td>
      <td>
        <%= link_to book.title, book_path(book.id) %>
        <!--遷移先はbooksのshowページ　/books/id-->
      </td>
      <td>
        <%= book.body %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
